{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "Build a JavaScript API based on a requirements given by the stakeholders. You will architect the database, tables, and columns to fulfill the requirements.",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "set ENV=test && db-migrate --env test up && npx tsc && jasmine && db-migrate db:drop test",
    "tsc": "tsc",
    "prettier": "prettier --config .prettierrc.json --write src/**/*.ts ",
    "lint": "eslint  src/**/*.ts  ",
    "db-up": "db-migrate up",
    "db-down": "db-migrate down",
    "build": "tsc --esModuleInterop src/server.ts --outDir ./dist",
    "nodemon": "npx tsc && nodemon src/server.ts"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "node_module": "^0.0.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "rimraf": "^4.0.5",
    "supertest": "^6.3.3",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "eslint": "^8.31.0",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.8.2",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.9.4"
  }
}
